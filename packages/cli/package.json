{
  "name": "@zhengyishen/basis-ui",
  "version": "0.1.1",
  "description": "Beautiful, customizable Astro + Alpine.js components that you can copy and paste into your apps. 59 components across 8 categories.",
  "type": "module",
  "bin": {
    "basis-ui": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.17",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "homepage": "https://basis.zhengyishen.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhengyishen0/basis-ui.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "astro",
    "alpine.js",
    "ui",
    "components",
    "cli",
    "basis",
    "tailwind",
    "copy-paste",
    "design-system",
    "shadcn",
    "static-site",
    "islands"
  ],
  "author": "Zhengyi Shen",
  "license": "MIT"
}
